<Window x:Class="MAP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MAP"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="loadFile1" Content="Load file" HorizontalAlignment="Left" Margin="33,30,0,0" VerticalAlignment="Top" Width="75" Click="testButton_Click"/>
        <TextBox x:Name="file1Dir" HorizontalAlignment="Left" Height="23" Margin="15,73,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="loadfile2" Content="Load second file" HorizontalAlignment="Left" Margin="178,30,0,0" VerticalAlignment="Top" Width="96" Click="Test2_Click"/>
        <TextBox x:Name="file2Dir" HorizontalAlignment="Left" Height="23" Margin="168,73,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="makeResult" Content="Test" HorizontalAlignment="Left" Margin="115,131,0,0" VerticalAlignment="Top" Width="75" Click="makeResult_Click"/>
        <TextBox x:Name="Result" HorizontalAlignment="Left" Height="23" Margin="95,161,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
